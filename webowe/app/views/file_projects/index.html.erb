<!DOCTYPE html>
<html>
<head>
  <title>Webowe</title>
  <%= stylesheet_link_tag    "main", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head><center>
<body>
<h1>Listing file_projects</h1>

<table style="width:480px;height:42px;z-index:2;" cellpadding="0" cellspacing="0" id="Table1" border="1">
<tr style="background-color:transparent;background-image:url(/images/table_head_bg.png);background-repeat:repeat-x;background-position:left top;border:1px #87CEEB solid;text-align:left;vertical-align:top;width:66px;height:18px;">
    <th>Source</th>
    <th>Info</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @file_projects.each do |file_project| %>
<tr style="background-color:#FFFFFF;border: 1px #87CEEB solid;text-align:left;vertical-align:top">
    <td><%= file_project.source %></td>
    <td><%= file_project.info %></td>
    <td><%= link_to 'Show', file_project %></td>
    <td><%= link_to 'Edit', edit_file_project_path(file_project) %></td>
    <td><%= link_to 'Destroy', file_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New File project', new_file_project_path %>
</center></body</html>